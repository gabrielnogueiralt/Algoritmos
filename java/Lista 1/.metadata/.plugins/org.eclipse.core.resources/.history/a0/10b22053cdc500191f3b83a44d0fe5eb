import java.util.Scanner;

public class Main {

	public static void main(String[] args) {
		Scanner in = new Scanner(System.in);
		int m = in.nextInt();
		String comando, expressao, temp = "", x = "", y = "";
		Stack stack = new Stack();
		
		for(int i = 0; i < m; i++) {
			comando = in.next();
			if (comando.equals("VAL")) {
				expressao = in.next();
				for(int j = 0; j < expressao.length();j++) {
					if(expressao.charAt(j) == '0' || expressao.charAt(j) == '1') {
						temp = Character.toString(expressao.charAt(j));
						stack.push(temp);
					} else if (expressao.charAt(j) == '~') {
						temp = stack.pop();
						if(temp.equals("0")) {
							temp = "1";
						} else {
							temp = "0";
						}
						stack.push(temp);
						
					} else if (expressao.charAt(j) == '^') {
						x = stack.pop();
						y = stack.pop();
						if (x.equals("1") && y.equals("1")){
							stack.push("1");
						} else {
							stack.push("0");
						}
					} else if (expressao.charAt(j) == 'v') {
						x = stack.pop();
						y = stack.pop();
						if (x.equals("1") || y.equals("1")){
							stack.push("1");
						} else {
							stack.push("0");
						}
					} else if (expressao.charAt(j) == '>') {
						x = stack.pop();
						y = stack.pop();
						if (x.equals("0") && y.equals("1")){
							stack.push("0");
						} else {
							stack.push("1");
						}
					}
				}
				
			} else {
				expressao = in.next();
			}
		}
		temp = stack.pop();
		System.out.println(temp);
	}

}
class Stack {
	public String[] stack;
	public int size;
	
	public Stack() {
		this.size = 0;
		this.stack = new String[100];
	}
	
	public String peek() {  
		if(isEmpty())  
			return null;  
		 
		return this.stack[getTopPosition()];  
	}
	
	private int getTopPosition() {
		if (isEmpty())
			return 0;
		return this.size - 1;  
	}
	
	public boolean isEmpty() {
		return this.size <= 0;
	}
	
	public String pop() {
		String result = peek();  

		if(result != null) {  
			stack[getTopPosition()] = null;  
			this.size--;  
		}  
		return result;
	}
	
	public void push(String value) {  
		this.size++;  
		this.stack[getTopPosition()] = value;    
	}  
}